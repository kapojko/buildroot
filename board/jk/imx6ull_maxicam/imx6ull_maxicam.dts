        >;
    };

    pinctrl_gpio_keys: gpiokeysgrp {
		fsl,pins = <
			MX6UL_PAD_GPIO1_IO03__GPIO1_IO03 0x1b010 /* Pull-down enabled, medium drive, hysteresis on. */
		>;
	};

    pinctrl_gpio_leds: gpioledsgrp {
        fsl,pins = <
            MX6UL_PAD_GPIO1_IO04__GPIO1_IO04 0x1b0b0 /* No pull (output mode), normal drive */
            MX6UL_PAD_GPIO1_IO05__GPIO1_IO05 0x1b0b0
            MX6UL_PAD_GPIO1_IO08__GPIO1_IO08 0x1b0b0
        >;
    };
};

/delete-node/ &sim2;
